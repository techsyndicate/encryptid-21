<link
  rel="stylesheet"
  href="https://cdn.jsdelivr.net/npm/notyf@3/notyf.min.css"
/>
<div class="load" id="loader">
  <div class="one"></div>
  <div class="two"></div>
  <div class="three"></div>
</div>
<div class="leaderboard-container">
  <div class="profile-heading"><%- level.name %> (<%- level.symbol %>)</div>
  <div class="play-wrapper">
    <div class="play-left">
      <div id="question"><%- level.question %></div>





      <!-- <%= level.sourceCodeHint %> -->






      <form id="main">
        <input
          type="text"
          name="answer"
          id="answer"
          autocomplete="off"
          placeholder="Answer"
        />
        <button type="submit" id="submit-button">Submit</button>
      </form>
    </div>
    <div class="play-right">
      <div class="stat-header">
        <span>Stats</span>
        <i class="fa fa-refresh" id="refresh"></i>
      </div>
      <div class="stats">
        <div class="stat">
          <span class="stat-label">Current Players</span><br />
          <span class="stat-value" id="curr-players"></span>
        </div>
        <div class="stat">
          <span class="stat-label">Current Value</span><br />
          <span class="stat-value" id="curr-value"></span>
        </div>
        <div class="stat">
          <span class="stat-label">ROI Rate</span><br />
          <span class="stat-value" id="roi-rate"></span>
        </div>
        <div class="stat">
          <span class="stat-label">Current ROI</span><br />
          <span class="stat-value" id="roi-value"></span>
        </div>
      </div>
    </div>
  </div>
</div>
<script src="https://cdn.jsdelivr.net/npm/notyf@3/notyf.min.js"></script>
<script src="/js/play.js"></script>
